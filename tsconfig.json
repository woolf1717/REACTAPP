{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "components/shop/shopcomponents/itemhorizontal.tsx",
    "pages/api/sendpost.mjss",
    "components/shop/itemslist/itemslist.js",
    "pages/api/getdata.ts",
    "pages/api/getusers.ts",
    "pages/products/[id].js",
    "components/shop/shopcomponents/itemvertical.jsx",
    "pages/api/getproducts.jsx",
    "components/menucomponents/loginForm.jsx",
    "pages/api/getusers.tsx",
    "components/newsfeed.tsx"
  ],
  "exclude": ["node_modules"],
  "useUnknownInCatchVariables": false
}
